<div class = "home">
    <h1 class="home_heading">Search Results</h1>
</div>


<section class="articles">
    <ul class = "article-ul">
        <li>

            <% data.forEach(post => { %>
             
            
            <a href="/post/<%= post.id  %>">
                <span><%= post.title  %></span>
                <span class="article-list_date"><%= post.createdAt.toDateString()  %></span>
            </a>
        </li>
        <% }) %>


    </ul> 
</section>